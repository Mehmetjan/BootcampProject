<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="NYT" parallel="tests">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="firefox"/>
    <parameter name="browserVersion" value="53.0"/>
    <parameter name="url" value="https://www.nytimes.com"/>

    <test name = "testNYT">
        <classes>
            <!--<class name="home.TestAmazonHome"/>-->
            <class name="testHomePage.TestSectionMenu"/>
            <class name ="testHomePage.TestPlayVideo"/>
            <class name = "testHomePage.TestShowWeather"/>
        </classes>
    </test>

</suite>